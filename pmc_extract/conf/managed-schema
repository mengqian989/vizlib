      <filter class="solr.LengthFilterFactory" min="2" max="5000"/>
      <filter class="solr.SynonymFilterFactory" expand="false" ignoreCase="true" synonyms="synonyms.txt" tokenizerFactory="solr.KeywordTokenizerFactory"/>
    </analyzer>
  </fieldType>
